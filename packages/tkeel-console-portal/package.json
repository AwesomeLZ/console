{
  "name": "@tkeel/console-portal",
  "version": "0.0.1",
  "homepage": "https://github.com/tkeel-io/console#readme",
  "bugs": {
    "url": "https://github.com/tkeel-io/console/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkeel-io/console.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "dev:base": "cross-env NODE_ENV=development webpack serve --config ../../webpack/webpack.config.js",
    "dev:admin": "cross-env PLATFORM=admin PORT=3000 yarn dev:base",
    "dev:tenant": "cross-env PLATFORM=tenant PORT=3001 yarn dev:base",
    "build:base": "cross-env NODE_ENV=production webpack --config ../../webpack/webpack.config.js",
    "build:admin": "cross-env PLATFORM=admin yarn build:base",
    "build:tenant": "cross-env PLATFORM=tenant yarn yarn:base",
    "analyze:admin": "cross-env PLATFORM=admin NODE_ENV=development ANALYZE=true yarn build:admin",
    "analyze:tenant": "cross-env PLATFORM=tenant NODE_ENV=development ANALYZE=true yarn build:tenant"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.7.3",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@tkeel/console-components": "workspace:^",
    "@tkeel/console-hooks": "workspace:^",
    "@tkeel/console-icons": "workspace:^",
    "@tkeel/console-themes": "workspace:^",
    "@tkeel/console-types": "workspace:^",
    "@tkeel/console-utils": "workspace:^",
    "framer-motion": "^5.5.5",
    "qiankun": "^2.6.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.22.5",
    "react-query": "^3.34.7",
    "react-router-dom": "^6.2.1"
  }
}
